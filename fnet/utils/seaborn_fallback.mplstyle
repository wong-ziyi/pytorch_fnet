# Minimal style used when Matplotlib's seaborn presets are unavailable.
axes.grid : True
axes.spines.top : False
axes.spines.right : False
axes.titlesize : 14
axes.labelsize : 12
figure.figsize : 6, 4
font.size : 11
legend.frameon : False
lines.linewidth : 2
xtick.labelsize : 10
ytick.labelsize : 10
